{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --config app.dev.json",
    "start:staging": "expo start --config app.staging.json",
    "start:production": "expo start --config app.production.json",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,tsx}'",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "storybook": "start-storybook -p 7007",
    "storybook:build": "build-storybook -o ./storybookPublic"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "dotenv": "^10.0.0",
    "expo": "~41.0.1",
    "expo-status-bar": "~1.0.4",
    "firebase": "8.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-web": "~0.16.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/react": "~17.0.11",
    "@types/react-native": "~0.64.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "~4.3.2"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix './src/**/*.{js,ts,tsx}'",
      "prettier --write ."
    ]
  }
}
